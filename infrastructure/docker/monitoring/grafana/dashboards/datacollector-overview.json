{
  "dashboard": {
    "id": null,
    "title": "DataCollector Overview",
    "tags": ["datacollector", "backend"],
    "style": "dark",
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Application Uptime",
        "type": "stat",
        "targets": [
          {
            "expr": "datacollector_uptime_seconds",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Memory Usage",
        "type": "stat",
        "targets": [
          {
            "expr": "datacollector_memory_used_bytes",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "bytes",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 6,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Request Rate",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(datacollector_requests_total[5m])",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 4,
        "title": "Job Status",
        "type": "timeseries",
        "targets": [
          {
            "expr": "datacollector_jobs_total{status=\"pending\"}",
            "legendFormat": "Pending",
            "refId": "A"
          },
          {
            "expr": "datacollector_jobs_total{status=\"running\"}",
            "legendFormat": "Running",
            "refId": "B"
          },
          {
            "expr": "datacollector_jobs_total{status=\"completed\"}",
            "legendFormat": "Completed",
            "refId": "C"
          },
          {
            "expr": "datacollector_jobs_total{status=\"failed\"}",
            "legendFormat": "Failed",
            "refId": "D"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 5,
        "title": "Service Health",
        "type": "timeseries",
        "targets": [
          {
            "expr": "datacollector_service_health",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "10s"
  }
} 