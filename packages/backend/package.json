{
  "name": "@datacollector/backend",
  "version": "1.0.0",
  "description": "DataCollector backend API server with AI-powered data collection capabilities",
  "main": "dist/app.js",
  "scripts": {
    "dev": "tsx watch src/app.ts",
    "build": "tsc",
    "start": "node dist/app.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:pipeline": "tsx src/tests/test-pipeline-simple.ts",
    "test:pubmed": "tsx src/tests/test-pubmed-scraper.ts",
    "test:arxiv": "tsx src/tests/test-arxiv-scraper.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "db:migrate": "tsx src/database/migrate.ts",
    "db:seed": "tsx src/database/seed.ts",
    "queue:dashboard": "npx bull-board",
    "setup": "npm run build && npm run db:migrate && npm run db:seed"
  },
  "dependencies": {
    "@bull-board/express": "^5.8.0",
    "@bull-board/ui": "^5.8.0",
    "@opensearch-project/opensearch": "^2.3.0",
    "axios": "^1.4.0",
    "bcryptjs": "^2.4.3",
    "bull": "^4.11.3",
    "cheerio": "^1.0.0-rc.12",
    "chromadb": "^1.5.3",
    "chromadb-default-embed": "^2.14.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.8.1",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.1",
    "langchain": "^0.3.30",
    "mammoth": "^1.6.0",
    "mime-types": "^2.1.35",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.2",
    "openai": "^5.10.2",
    "pdf-parse": "^1.1.1",
    "pg": "^8.11.1",
    "playwright": "^1.36.2",
    "redis": "^4.6.7",
    "socket.io": "^4.7.2",
    "uuid": "^9.0.0",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/bull": "^4.10.0",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/mime-types": "^2.1.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.4.5",
    "@types/node-cron": "^3.0.8",
    "@types/pdf-parse": "^1.1.1",
    "@types/pg": "^8.10.2",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.2",
    "jest": "^29.6.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "tsx": "^3.12.7",
    "typescript": "^5.1.6"
  },
  "keywords": [
    "express",
    "typescript",
    "api",
    "ai",
    "data-collection",
    "web-scraping",
    "opensearch",
    "redis",
    "bull"
  ],
  "author": "DataCollector Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  }
}
